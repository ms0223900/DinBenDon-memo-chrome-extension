{
    "name": "DinBenDan Memo Plugin",
    "description": "DinBenDon Memo Plugin",
    "version": "1.1",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "tabs",
        "storage"
    ],
    "action": {
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": ["https://www.dinbendon.net/*"],
            "js": ["content-script.js"]
        }
    ]
}